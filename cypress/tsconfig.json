{
    "compilerOptions": {
        "lib": ["DOM", "DOM.Iterable", "ES2019"],
        "types": ["cypress", "node"],
        "jsx": "react-jsx",
        "target": "ES2019",
        "module": "ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowJs": true,
        "baseUrl": ".",
        "paths": {
            "~/*": ["./app/*"]
        },
        "noEmit": true
    }
}
